<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 불러올 Data 별명 -->
<mapper namespace="Member">
	
	<!-- 사용자 가입 -->
	<insert id="register" parameterType="Member">
		INSERT INTO 
			ADMIN
		(eMail, passWord) 
			VALUES 
		(#{eMail},#{passWord})
	</insert>
	
</mapper>
    
    